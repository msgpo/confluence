{{#each data.space as |space|}}
    {{#if space.space.name}}
    <div>
        <span class="p-key">Space: </span>
        <span class ="p-value"><a href ="{{data.url}}{{space.space.url}}"> {{space.space.name}} </a></span>
    </div>
    {{/if}}
{{/each}}

{{#each data.page as |page|}}
  {{#if page.content.title}}
    <div>
        <span class="p-key"> Page: </span>
        <span class ="p-value"><a href ="{{data.url}}{{page._links.webui}}"> {{page.content.title}} </a></span>
  </div>
  {{/if}}
{{/each}}

{{#each data.blog as |blog|}}
    {{#if blog.content.title}}
    <div>
        <span class="p-key">Blog: </span>
        <span class="p-value"><a href ="{{data.url}}{{blog._links.webui}}"> {{blog.content.title}} </a></span>
      </div>
  {{/if}}
{{/each}}
